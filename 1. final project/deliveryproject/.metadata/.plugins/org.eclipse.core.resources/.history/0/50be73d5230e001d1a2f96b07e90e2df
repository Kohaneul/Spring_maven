function radioClick(no){
	document.getElementById('no').value = no;
}



function onClick(){

	let no = document.getElementById('no').value;
	let place_name = document.getElementById('place_name').value;
	let code = document.getElementById('code').value;
	if(!no){
			alert("다시입력해주세요");
			location.reload = '/deli/place/restaurant.dlv?place_name='+place_name+'&code='+code;	
			return;
			}
						/**
		//	deli/place/restaurant.dlv?place_name=약수역&code=1010
	
		}
		 */
}